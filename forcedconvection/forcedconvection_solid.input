# Settings file for numsim program
# Run ./numsim driven_cavity.txt

# Problem description
geometryFile = forcedconvection_solid_20x10.geom
endTime = 10.0        # duration of the simulation
participantName = SolidSolver
meshName = SolidSolverMesh
preciceConfigFile = precice-config-forcedconvection.xml
readDataName = Temperature
writeDataName = Heat-Flux

heatDiffusivity = 0.2
tInit = 0.2

# Discretization parameters
maximumDt = 1.0       # maximum values for time step width

# Solver parameters
#temperatureSolver = GaussSeidel
temperatureSolver = SOR
omega = 1.5
epsilon = 1e-5        # tolerance for 2-norm of residual
maximumNumberOfIterations = 1e4    # maximum number of iterations in the solver

outputFileEveryDt = 0.05
