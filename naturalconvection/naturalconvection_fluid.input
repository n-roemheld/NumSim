# Settings file for numsim program
# Run ./numsim driven_cavity.txt

# Problem description
geometryFile = naturalconvection_fluid_25x25.geom
endTime = 10.0        # duration of the simulation
re = 1000             # Reynolds number
prandtl = 0.01 # Prandtl number
beta = 0.2 # Volumetric expansion coefficient beta = Ri / (g * Re^2)
gX = 0.    # external forces, set to (gX,gY) = (0,-9.81) to account for gravity
gY = -9.81
participantName = FluidSolver
meshName = FluidSolverMesh
preciceConfigFile = precice-config-naturalconvection-implicit.xml
readDataName = Heat-Flux
writeDataName = Temperature


uInit = 0.
vInit = 0.
pInit = 0.
tInit = 303.0

# Discretization parameters
useDonorCell = true   # if donor cell discretization should be used, possible values: true false
alpha = 0.9           # factor for donor-cell scheme, 0 is equivalent to central differences
gamma = 0.5           # factor for donor-cell scheme (temperature), 0 is equivalent to central differences
tau = 0.5             # safety factor for time step width
maximumDt = 0.01       # maximum values for time step width

# Solver parameters
pressureSolver = SOR  # which pressure solver to use, possible values: GaussSeidel SOR CG
omega = 1.7           # overrelaxation factor, only for SOR solver
epsilon = 1e-5       # tolerance for 2-norm of residual
maximumNumberOfIterations = 1e5    # maximum number of iterations in the solver

outputFileEveryDt = 0.01
